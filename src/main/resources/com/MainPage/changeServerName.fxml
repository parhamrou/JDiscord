<AnchorPane prefHeight="100.0" prefWidth="400.0" style="-fx-background-color: #616161;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.MainPage.ChangeServerNameController">
   <children>
      <Label layoutX="23.0" layoutY="14.0" text="CHANGE  NAME" textFill="#bfbfbf">
