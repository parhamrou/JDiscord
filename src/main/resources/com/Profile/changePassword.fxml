<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #616161;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Profile.ChangePasswordController">
   <children>
      <Label layoutX="125.0" layoutY="21.0" style="-fx-background-color: #616161;" text="CHANGE PASSWORD" textFill="#a9a9a9">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="firstTextField" layoutX="64.0" layoutY="59.0" prefHeight="30.0" prefWidth="272.0" promptText="Enter password" style="-fx-background-color: #3D3D3D; -fx-text-fill: #b6b6b2;" />
      <PasswordField fx:id="secondTextField" layoutX="64.0" layoutY="108.0" prefHeight="30.0" prefWidth="272.0" promptText="Enter password again" style="-fx-background-color: #3D3D3D; -fx-text-fill: #b6b6b2;" />
      <Button fx:id="button" layoutX="64.0" layoutY="160.0" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="272.0" style="-fx-background-color: #4d4dff;" text="Change" textFill="#bfbfbf">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Text fx:id="alertText" layoutX="64.0" layoutY="152.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
         <font>
            <Font size="11.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
